<UserControl x:Name="propertiesControl" x:Class="SRPSimulator.PropertiesControl.PropertiesControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SRPSimulator.PropertiesControl"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="310" Loaded="propertiesControl_Loaded">
    <Grid>
        <WindowsFormsHost >
            <local:PropertiesPanel x:Name="Properties" Margin="0,0,0,0" />
        </WindowsFormsHost>
    </Grid>
</UserControl>
